{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/project-link",
	"version": "1.0.0",
	"title": "Project Link",
	"category": "portfolio",
	"icon": "cover-image",
	"description": "Link to one of the project pages with a featured image and CTA",
	"supports": {
		"html": false
	},
	"textdomain": "project-link",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",

	"attributes": {
		"description": {
			"type": "string",
			"source": "text",
			"selector": "div",
			"default": ""
		},
		"imageUrl": {
			"type": "string"
		},
			"imageId": {
			"type": "number"
		},
		"imageAlt": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "alt",
			"default": ""
		}
	}
}