{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "dwd-block-loader/project-link",
	"version": "1.0.0",
	"title": "Project Link",
	"category": "portfolio",
	"icon": "cover-image",
	"description": "Link to one of the project pages with a featured image and CTA",
	"supports": {
		"html": false
	},
	"textdomain": "project-link",
	"attributes": {
		"title": {
		  "type": "string",
		  "source": "text",
		  "selector": ".title"
		},
		"logoID": {
			"type": "number"
		},
		"logoURL": {
			"type": "string",
			"source": "attribute",
			"attribute": "src",
			"selector": ".logo-img"
		},
		"ftImgID": {
			"type": "number"
		},
		"ftImgURL": {
		  "type": "string",
		  "source": "attribute",
		  "attribute": "src",
		  "selector": ".ft-img"
		},
		"textDescription": {
		  "type": "string",
		  "source": "html",
		  "selector": ".text-description"
		},
		"link": {
		  "type": "string",
		  "source": "attribute",
		  "attribute": "href",
		  "selector": ".project-link-body-cta a"
		},
		"linkTitle": {
			"type": "string",
			"source": "text",
			"selector": ".project-link-body-cta a"
		  }
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}